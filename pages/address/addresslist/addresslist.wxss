page {
  height: 100%;
  width: 100%;
}

.container-all {
  background-color: #f5f5f5;
}


.item-box {
  background-color: #fff;
  color: #333;
}

.box-header {
  font-size: 30rpx;
  height: 92rpx;
  margin-left: 30rpx;
  line-height: 92rpx;
}

.tel {
  position: absolute;
  right: 30rpx;
}

.detail-address {

  font-size: 32rpx;
  margin-left: 30rpx;
  border-bottom: solid 1rpx #e8e8e8;
  padding-bottom: 20rpx;
    padding-right: 20rpx;
}

.box-bottom {
  background-color: #fff;
  font-size: 30rpx;
  color: #333;
  height: 108rpx;
  margin-bottom: 20rpx;
}

.item-del {
  padding: 9rpx 40rpx;
  border-radius: 8rpx;
  border: solid 1rpx #979797;
  margin: 24rpx 10rpx;
  position: absolute;
  right: 190rpx;
}

.item-edit {
  position: absolute;
  right: 30rpx;
  border: solid 1rpx #f53;
  color: #f53;
}

.new-address {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  background-color: #f53;
  position: fixed;
  bottom: 0;
}