.container-all {
  background-color: #f5f5f5;
  height: 100%;
  padding: 15rpx;
  margin: 0;
}

.recycler-box {
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin-bottom: 1000rpx;
}

.recycler-item {
  margin: 15rpx;
  text-align: center;
  height: 100rpx;
  width: 325rpx;
    font-size: 36rpx;
  line-height: 100rpx;
  background-color: #fff;
    color: #333333;
}
.recycler-itemed{
    margin: 15rpx;
  text-align: center;
  height: 100rpx;
  font-size: 36rpx;
  width: 325rpx;
  line-height: 100rpx;
  background-color: #ff5533;
    color: #ffffff;
}

.bt-confirmed {
  position: fixed;
  left: 0;
  bottom: 0;
  font-size: 36rpx;
  color: #333;
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  text-align: center;
  z-index: 100;
  background-color: #fff;
}
